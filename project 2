#Q1
print ("Q1")
count = int(input("Enter a number: "))
while count > 0:
    print (count)
    count -= 1
print ("Blast off!")

#Q2
print()
print ("Q2")
count = int(input("Enter a number: "))
for i in range (10): 
    print (count * i)
print ("Done!")

#Q3
print()
print ("Q3")
count = int(input("Enter a number: "))
factorial = 1

for i in range(1, count + 1):
    factorial *= i

print(f"The factorial of {count} is {factorial}.")